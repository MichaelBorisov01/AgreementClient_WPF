<Window x:Class="AgreementClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AgreementClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem x:Name="Person" Header="Клиенты"
Click="Person_OnClick"></MenuItem>
            <MenuItem x:Name="Agreement" Header="Договоры" 
                      Click="Agreement_OnClick"></MenuItem>
            <MenuItem x:Name="TypeAgreement" Header="Тип договора"
Click="TypeAgreement_OnClick"></MenuItem>
            <MenuItem x:Name="StatusAgreement" Header="Статус договора"
Click="StatusAgreement_OnClick"></MenuItem>
        </Menu>
    </Grid>
</Window>
